              {
                "name": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "json",
              "json": {
                "email": "empresa@ejemplo.com",
                "password": "password123"
              }
            }
          }
        },
        {
          "name": "Register Cliente",
          "request": {
            "method": "POST",
            "url": "http://localhost:3000/auth/register/cliente",
            "headers": [
              {
                "name": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "json",
