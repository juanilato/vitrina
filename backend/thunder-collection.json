        {
          "name": "Register Empresa",
          "request": {
            "method": "POST",
            "url": "http://localhost:3000/auth/register/empresa",
            "headers": [
              {
                "name": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "json",
              "json": {
                "email": "nueva@empresa.com",
                "name": "Nueva Empresa",
                "password": "password123",
                "logo": "https://ejemplo.com/logo.png"
              }
            }
          }
        },
        {
          "name": "Refresh Token",
          "request": {
            "method": "POST",
            "url": "http://localhost:3000/auth/refresh",
            "headers": [
              {
                "name": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "json",
              "json": {
                "refreshToken": "{{refreshToken}}"
              }
            }
          }
        }
      ]
    },
    {
      "name": "Protected Routes",
      "items": [
        {
          "name": "Get Profile",
          "request": {
            "method": "GET",
            "url": "http://localhost:3000/auth/profile",
            "headers": [
              {
                "name": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ]
          }
        },
        {
          "name": "Logout",
          "request": {
            "method": "POST",
            "url": "http://localhost:3000/auth/logout",
            "headers": [
              {
                "name": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ]
          }
        }
      ]
    }
  ],
